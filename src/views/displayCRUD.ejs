<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>

<body>
    <table class="table">
        <thead>
            <tr>
                <th scope="col">STT</th>
                <th scope="col">Email</th>
                <th scope="col">Tên</th>
                <th scope="col">Họ</th>
                <th scope="col">Địa chỉ</th>
                <th scope="col">Số điện thoại</th>
                <th scope="col">Vai trò</th>
                <th scope="col">Hành động</th>
            </tr>
        </thead>
        <tbody>
            <% for(let i=0; i < duLieu.length; i++) { %>
                <tr>
                    <td>
                        <%= duLieu[i].id %>
                    </td>
                    <td>
                        <%= duLieu[i].email %>
                    </td>
                    <td>
                        <%= duLieu[i].ho %>
                    </td>
                    <td>
                        <%= duLieu[i].ten %>
                    </td>
                    <td>
                        <%= duLieu[i].diaChi %>
                    </td>
                    <td>
                        <%= duLieu[i].soDienThoai %>
                    </td>
                    <td>
                        <%= duLieu[i].vaiTro %>
                    </td>
                    <td>
                        <a href="/edit-crud?id=<%= duLieu[i].id %>" type="button">
                            <button class="button">Sửa</button></a>
                        <a href="/delete-crud?id=<%= duLieu[i].id %>" type="button">
                            <button class="button">Xóa</button></a>
                    </td>
                </tr>
                <%} %>
        </tbody>
    </table>
</body>

</html>